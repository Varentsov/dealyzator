<h1>Users#index</h1>
<table class="table">
  <tr>
		
    <th>First name</th>
    <th>Last name</th>
    <th>Email</th>
    <th>ID</th>
  
  </tr>

<% @users.each do |user| %>
  <tr >
		
    <td><%= user.firstname %></td>
    <td><%= user.lastname %></td>
    <td><%= user.email %></td>
    <td><%= user.id %></td>
   

  </tr>
<% end %>
</table>
